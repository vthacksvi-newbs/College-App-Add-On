{
    "name": "College Applications",
    "version": "1.0",
    "description": "Save college information to an extension",
    "manifest_version": 2,

    "content_scripts": [
        {
            "js": [
                "xmlParser.js",
                "background.js"
            ],
            "matches": [
                "https://*/*",
                "file://*"
            ],
            "run_at": "document_end"
        }
    ],
    
    "web_accessible_resources": [ 
        "xmlFile.xml"
    ] 
}